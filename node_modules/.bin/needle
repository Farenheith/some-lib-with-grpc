#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/thiago/projects/personal/some-lib-with-grpc/node_modules/.pnpm/needle@2.6.0/node_modules/needle/bin/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules/.pnpm/needle@2.6.0/node_modules/needle/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules/.pnpm/needle@2.6.0/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules/.pnpm/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules:/home/thiago/projects/personal/node_modules:/home/thiago/projects/node_modules:/home/thiago/node_modules:/home/node_modules:/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules/needle/bin/node_modules:/home/thiago/projects/personal/some-lib-with-grpc/node_modules/needle/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
